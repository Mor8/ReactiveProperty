<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="MSBuild.Sdk.Extras">
  <PropertyGroup>
    <TargetFramework>xamarinios10</TargetFramework>
    <RootNamespace>Reactive.Bindings</RootNamespace>
    <PackageId>ReactiveProperty.XamariniOS</PackageId>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <!-- 
    Library layout generation is disabled as this project does not have any XAML files, and 
    that if set to true, there is a nuget issue where Uno.Prism PRI resource get incorrectly
    propagated to their dependents.
    -->
    <GenerateLibraryLayout>false</GenerateLibraryLayout>
    <AssemblyName>ReactiveProperty.XamariniOS</AssemblyName>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>key.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\ReactiveProperty.NETStandard\ReactiveProperty.NETStandard.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\ReactiveProperty.Platform.Shared\Version.cs" Link="Version.cs" />
  </ItemGroup>
</Project>

